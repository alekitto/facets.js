        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^11.0.1",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "autoprefixer": "^10.1.0",
        "eslint": "^7.14.0",
        "fantasticon": "^1.0.25",
